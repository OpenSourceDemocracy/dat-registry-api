{
  "name": "dat-registry-api",
  "version": "3.0.0",
  "description": "An online place for dats.",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/datproject/datproject.org.git"
  },
  "bugs": {
    "url": "https://github.com/datproject/datproject.org/issues"
  },
  "main": "server/index.js",
  "bin": "server/cli.js",
  "browser": {
    "server/**/**/*": false
  },
  "browserify": {
    "transform": [
      [
        "sheetify/transform",
        {
          "use": "sheetify-nested"
        }
      ]
    ]
  },
  "scripts": {
    "lint": "standard",
    "database": "node ./server/api/database/init",
    "clean-test": "rm -rf tests/archiver && rm -rf tests/township.db && rm -rf tests/unit/*.sqlite",
    "test": "standard && npm run clean-test && NODE_ENV=test tape tests/*.js | tap-spec"
  },
  "devDependencies": {
    "rimraf": "^2.5.4",
    "standard": "^7.1.2",
    "tap-spec": "^4.1.1",
    "tape": "^4.6.3"
  },
  "dependencies": {
    "body-parser": "^1.15.2",
    "dat-encoding": "^4.0.1",
    "envify": "^4.0.0",
    "knex": "^0.12.6",
    "level-party": "^3.0.4",
    "mixpanel": "^0.7.0",
    "nodemailer-mock-transport": "^1.3.0",
    "nodemailer-postmark-transport": "^1.1.0",
    "pump": "^1.0.2",
    "random-access-file": "^1.5.0",
    "random-access-memory": "^2.4.0",
    "range-parser": "^1.2.0",
    "response": "^0.18.0",
    "sqlite3": "^3.1.8",
    "township": "^2.0.0",
    "township-reset-password-token": "^1.0.0",
    "uparams": "^1.3.1",
    "uuid": "^3.0.1",
    "xtend": "^4.0.1"
  }
}
